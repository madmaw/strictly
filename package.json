{
  "devDependencies": {
    "@dprint/dockerfile": "=0.3.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "cross-env": "^7.0.3",
    "del-cli": "^6.0.0",
    "eslint": "^9.13.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-dprint-integration": "^0.3.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-no-autofix": "^2.0.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-yml": "^1.15.0",
    "tsup": "^8.3.5",
    "turbo": "^2.2.3",
    "type-fest": "^4.26.1",
    "typescript": "~5.5.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3",
    "vitest-mock-extended": "^2.0.2"
  },
  "license": "MIT",
  "main": "index.js",
  "name": "@de/de",
  "packageManager": "yarn@1.22.22",
  "private": true,
  "resolutions": {
    "**/**/@dprint/dockerfile": "=0.3.0",
    "**/**/vite": "=5.4.10"
  },
  "scripts": {
    "build": "turbo run build",
    "build:force": "turbo run build --force",
    "check-types": "turbo run check-types",
    "check-types:force": "turbo run check-types --force",
    "clean": "turbo run clean",
    "demo:dev": "turbo run dev --filter=@de/mo",
    "demo:storybook": "turbo storybook --filter=@de/mo",
    "form-react:storybook": "turbo storybook --filter=@de/form-react",
    "lint": "turbo run lint --continue && yarn lint:root",
    "lint:fix": "turbo run lint:fix --continue && yarn lint:root:fix",
    "lint:root": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint --max-warnings=0 *.cjs *.json .github",
    "lint:root:fix": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint --fix *.cjs *.json .github",
    "test": "turbo run test",
    "test:ci": "turbo run test",
    "test:force": "turbo run test --force"
  },
  "version": "0.0.1",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "support/*"
    ]
  }
}
