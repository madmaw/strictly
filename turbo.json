{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "always": {
    },
    "build": {
      "dependsOn": [
        "^build",
        "^check-types"
      ],
      "outputs": ["dist/**"]
    },
    "check-types": {
      "dependsOn": [
        "^always",
        "^check-types"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^check-types"]
    },
    "lingui:extract": {
    },
    "lint": {
      "dependsOn": ["^check-types"]
    },
    "release:exports": {
    },
    "storybook": {
      "dependsOn": ["^check-types"]
    },
    "storybook:test": {
      "dependsOn": ["^check-types"]
    },
    "test": {
      "dependsOn": ["^check-types"]
    },
    "test:update": {
      "dependsOn": ["^check-types"]
    }
  }
}
